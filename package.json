{
  "name": "ember-ast-hot-load",
  "version": "0.1.1",
  "description": "Ember components hot-reload addon",
  "keywords": [
    "ember-addon",
    "ast",
    "dev-tools",
    "hot-loading",
    "hot-reloading",
    "development",
    "reloading",
    "live-reload"
  ],
  "repository": "https://github.com/lifeart/ember-ast-hot-load",
  "license": "MIT",
  "author": "Aleksandr Kanunnikov <lifeart92@gmail.com>, Toran Billups <toranb@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:jest": "jest --no-watchman",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "babel-plugin-ember-modules-api-polyfill": ">=2.0.1",
    "broccoli-stew": "^3.0.0",
    "ember-cli-babel": ">=7.12.0",
    "ember-cli-htmlbars": "*"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.1.0",
    "@glimmer/syntax": "0.47.4",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.13.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-fastboot": "^2.2.1",
    "ember-cli-htmlbars-inline-precompile": "^3.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^5.3.0",
    "ember-source": "~3.16.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.3.0",
    "eslint-plugin-ember": "^7.5.0",
    "jest": "24.9.0",
    "eslint-plugin-node": "^10.0.0",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.9.1"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-template-component-import",
      "ember-holy-futuristic-template-namespacing-batman",
      "ember-template-compiler"
    ],
    "before": [
      "proxy-server-middleware",
      "ember-angle-bracket-invocation-polyfill",
      "ember-let-polyfill"
    ]
  }
}
