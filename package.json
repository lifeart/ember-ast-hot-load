{
  "name": "ember-ast-hot-load",
  "version": "1.2.3",
  "description": "Ember components hot-reload addon",
  "keywords": [
    "ember-addon",
    "ast",
    "dev-tools",
    "hot-loading",
    "hot-reloading",
    "development",
    "reloading",
    "live-reload"
  ],
  "repository": "https://github.com/lifeart/ember-ast-hot-load",
  "license": "MIT",
  "author": "Aleksandr Kanunnikov <lifeart92@gmail.com>, Toran Billups <toranb@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:jest": "jest --no-watchman"
  },
  "dependencies": {
    "babel-plugin-ember-modules-api-polyfill": ">=2.0.1",
    "broccoli-stew": "^3.0.0",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "*"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/syntax": "0.71.0",
    "@glimmer/tracking": "^1.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.20.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.20.2",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.10.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.5.0",
    "eslint-plugin-ember": "^8.9.1",
    "eslint-plugin-node": "^11.1.0",
    "jest": "26.4.2",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.2.0",
    "symlink-or-copy": "1.3.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-template-component-import",
      "ember-holy-futuristic-template-namespacing-batman",
      "ember-template-compiler"
    ],
    "before": [
      "proxy-server-middleware",
      "ember-angle-bracket-invocation-polyfill",
      "ember-let-polyfill"
    ]
  }
}
